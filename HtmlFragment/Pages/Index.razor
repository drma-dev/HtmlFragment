@page "/"
@inject VariableHelper VariableHelper
@inject IMatDialogService MatDialogService

<h4 style="margin-top: 20px; margin-bottom: 20px;">RenderFragment</h4>

@{ var fragment = @VariableHelper.CreateGenericComponent(html, this, MatDialogService); }

<div>
    @fragment
</div>

<h4 style="margin-top: 20px; margin-bottom: 20px;">MarkupString</h4>

@((MarkupString)html)

@code
{
    public string html { get; set; } = "<p class='ql-align-justify'>{@button01} <span style='color: rgb(230, 0, 0);'>123 </span><strong style='color: rgb(255, 153, 0); background-color: rgb(0, 138, 0);'>abc</strong></p>";
}